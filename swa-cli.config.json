{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "workflow": {
      "appLocation": "/",
      "appBuildCommand": "npm run b:swa:ui",
      "apiLocation": "code/api",
      "apiBuildCommand": "npm run build:swa",
      "outputLocation": "code/ui/dist"
    },
    "dev": {
      "apiDevserverUrl": "http://localhost:9091",
      "apiLanguage": "node",
      "apiLocation": "code/api",
      "apiVersion": "18",
      "appDevserverUrl": "http://localhost:9090",
      "appLocation": ".",
      "outputLocation": "code/ui/dist",
      "run": "npm run dev"
    },
    "serve": {
      "apiLanguage": "node",
      "apiLocation": "code\\api",
      "apiVersion": "18",
      "appLocation": "code\\ui\\dist"
    },
    "api": {
      "apiLanguage": "node",
      "apiVersion": "18",
      "run": "npm run start -w @noodles-house/api",
      "apiDevserverUrl": "http://localhost:9091"
    },
    "attach": {
      "appDevserverUrl": "http://localhost:9090",
      "apiDevserverUrl": "http://localhost:9091"
    }
  }
}
