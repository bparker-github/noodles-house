{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "noodles-house": {
      "appLocation": ".",
      "apiLocation": ".",
      "outputLocation": "modules\\ui\\dist",
      "apiLanguage": "node",
      "apiVersion": "18",
      "appBuildCommand": "npm run build:ui",
      "apiBuildCommand": "npm run build:api --if-present",
      "run": "npm run dev",
      "appDevserverUrl": "http://localhost:9090",
      "apiDevserverUrl": "http://localhost:9091"
    }
  }
}
