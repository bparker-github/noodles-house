{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "dev": {
      "apiDevserverUrl": "http://localhost:3001",
      "apiLanguage": "node",
      "apiLocation": "api",
      "apiVersion": "18",
      "dataApiLocation": "swa-db-connections",
      "appDevserverUrl": "http://localhost:3000",
      "appLocation": ".",
      "verbose": "true"
    },
    "build": {
      "apiBuildCommand": "npm run build",
      "apiLanguage": "node",
      "apiLocation": "api",
      "apiVersion": "18",
      "appBuildCommand": "npm run build.ui",
      "appLocation": ".",
      "outputLocation": "ui",
      "verbose": "true"
    }
  }
}
