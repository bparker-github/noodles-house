    "@vitejs/plugin-vue": "^4.3.4",
    "pinia": "^2.1.6",
    "unsplash-js": "^7.0.18",
    "vite": "^4.4.9",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
