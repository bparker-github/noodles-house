{
  "name": "@noodles-house/ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all -lns build:types build:code",
    "build:local": "npm-run-all -lns build:types build:code:local",
    "build:swa": "npm-run-all -lns build:types build:code:swa",
    "build:swa-local": "npm-run-all -lns build:types build:code:swa-local",
    "build:code": "vite build --mode swa",
    "build:code:local": "vite build --mode local",
    "build:code:swa": "vite build --mode swa",
    "build:code:swa-local": "vite build --mode swa-local",
    "build:types": "vue-tsc -p ./tsconfig.app.json --declaration --noEmit",
    "preview": "vite preview",
    "test:unit": "vitest",
    "util:format": "prettier --write src/",
    "util:lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@noodles-house/db": "^0.1.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@vueuse/core": "^10.5.0",
    "blurhash": "^2.0.5",
    "date-fns": "^3.6.0",
    "lorem-ipsum": "^2.0.8",
    "pinia": "^2.1.7",
    "unsplash-js": "^7.0.18",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  }
}
