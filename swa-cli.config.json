{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "build": {
      "apiBuildCommand": "npm run build:swa:api",
      "apiLanguage": "node",
      "apiLocation": "api",
      "apiVersion": "18",
      "appBuildCommand": "npm run build:swa:ui",
      "appLocation": "/",
      "outputLocation": "ui\\dist",
      "dataApiLocation": "api\\src\\database"
    },
    "build:api": {
      "apiLanguage": "node",
      "apiLocation": "api",
      "apiVersion": "18",
      "apiBuildCommand": "npm run build:swa:api",
      "dataApiLocation": "api\\src\\database"
    },
    "build:ui": {
      "appLocation": "/",
      "outputLocation": "ui\\dist",
      "appBuildCommand": "npm run build:swa:ui",
      "dataApiLocation": "api\\src\\database"
    },
    "dev": {
      "appDevserverUrl": "http://localhost:9090",
      "appLocation": "/",
      "outputLocation": "ui\\dist",
      "apiDevserverUrl": "http://localhost:9091",
      "apiLanguage": "node",
      "apiLocation": "api",
      "apiVersion": "18",
      "run": "npm run dev",
      "dataApiLocation": "api\\src\\database"
    },
    "dev:api": {
      "apiDevserverUrl": "http://localhost:9091",
      "apiLanguage": "node",
      "apiLocation": "api",
      "apiVersion": "18",
      "run": "npm run dev:api",
      "dataApiLocation": "api\\src\\database"
    },
    "dev:ui": {
      "appDevserverUrl": "http://localhost:9090",
      "appLocation": ".",
      "outputLocation": "ui\\dist",
      "run": "npm run dev:ui",
      "dataApiLocation": "api\\src\\database"
    },
    "dev:debug": {
      "appDevserverUrl": "http://localhost:9090",
      "apiDevserverUrl": "http://localhost:9091"
    },
    "serve": {
      "apiLanguage": "node",
      "apiLocation": "api",
      "apiVersion": "18",
      "appLocation": "ui\\dist",
      "dataApiLocation": "api\\src\\database"
    },
    "serve:api": {
      "apiLanguage": "node",
      "apiLocation": "api",
      "apiVersion": "18",
      "dataApiLocation": "api\\src\\database"
    },
    "serve:ui": {
      "appLocation": "ui\\dist",
      "dataApiLocation": "api\\src\\database"
    }
  }
}
